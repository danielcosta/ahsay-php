{
  "name": "ahsay/php-sdk",
  "description": "Ahsay PHP SDK for OBS and CBS",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Ahsay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ahsay\\Test\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Daniel Costa",
      "email": "danielcosta@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "ext-dom": "*",
    "phpstan/phpstan": "^0.11.5",
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "^7.0",
    "php-mock/php-mock-phpunit": "^2.2"
  }
}
